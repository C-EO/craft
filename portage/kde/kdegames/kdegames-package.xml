<?xml version="1.0" encoding="UTF-8"?>
<module name="kdegames">
    <package name="libkdegames">
        <shortDescription>the kdegames support libraries</shortDescription>
		<part name="runtime">
			<files include="bin/.*(kdegames|kggznet|kggzgames|kggzmod|kmahjongglib).*"/>
			<files include="share/.*(carddecks|kdegames|kmahjongglib).*"/>
		</part>
		<part name="development">
			<files include="lib/.*(kdegames|kggznet|kggzgames|kggzmod|kmahjongglib|kcardgame).*"/>
			<files include="lib/(?!kde4).*(pala).*(?!dll)"/>
			<files include="include/*"/>
			<files include="share/.*cmake.*"/>
		</part>
        <dependency>kde/kde-runtime</dependency> 
        <dependency>kde/kdelibs</dependency>
    </package>
